{
  "builds": [
    {
      "src": "api/saveData.js",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/saveData"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
